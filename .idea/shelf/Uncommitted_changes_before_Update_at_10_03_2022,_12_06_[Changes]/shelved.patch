+/**
+ * This class is an example route that gets the initials of the given first, last and middle names
+ *
+ * @author Jaden van Rijswijk
+ */
+
+class getInitialsRoute {
+    #errorCodes = require("../framework/utils/httpErrorCodes")
+    #app
+
+    constructor(app) {
+        this.#app = app;
+
+        this.#getInitials();
+    }
+
+    #getInitials() {
+        this.#app.get("/get_initials/:namesID", async (req, res) => {
+            let names = (req.params.namesID).split(" ")
+            let initials = names.map((name) => name[0].toUpperCase()).join(".")
+
+            try {
+                res.status(this.#errorCodes.HTTP_OK_CODE).json("Welkom " + initials)
+            }
+            catch (e) {
+                res.status(this.#errorCodes.BAD_REQUEST_CODE).json({reason: e})
+            }
+        })
+    }
+}
+
+module.exports = getInitialsRoute
